--데이터 조작어
INSERT INTO BOARD(NUM, TITLE, AUTHOR, CONTENT)
VALUES(1,'테스트','홍길동','테스트입니다');
SELECT* FROM BOARD;
------------------------------------------------------------------------
INSERT INTO DEPT(DEPTNO, DNAME, LOC)
VALUES(45,'개발부','서울');
SELECT*FROM DEPT;

INSERT INTO EMP(EMPNO, ENAME, DEPTNO)
VALUES(6789,'홍길동',70);
------------------------------------------------------------------------
UPDATE DEPT
SET DNAME='기획실'
WHERE DEPTNO = 50;
SELECT*FROM DEPT;

DELETE FROM DEPT
WHERE DEPTNO = 50;
SELECT * FROM DEPT;

DELETE FROM DEPT
WHERE DEPTNO = 30;
------------------------------------------------------------------------
INSERT INTO DEPT(DEPTNO, DNAME, LOC)
VALUES(50,'인사과','부산');
COMMIT;
SELECT*FROM DEPT;
------------------------------------------------------------------------
DELETE FROM EMP;
SELECT*FROM EMP;
ROLLBACK;
SELECT*FROM EMP;
------------------------------------------------------------------------
SELECT*FROM DEPT;

INSERT INTO DEPT(DEPTNO, DNAME, LOC) VALUES(60, '총무과', '광주');
--1 행 이(가) 삽입되었습니다.
--SELECT*FROM DEPT;
SAVEPOINT A;
--Savepoint이(가) 생성되었습니다.
INSERT INTO DEPT(DEPTNO, DNAME, LOC) VALUES(70, '개발과', '대구');
--1 행 이(가) 삽입되었습니다.
--SELECT*FROM DEPT;
SAVEPOINT B;
--Savepoint이(가) 생성되었습니다.

DELETE FROM DEPT WHERE DEPTNO = 60 OR DEPTNO = 70;
--2개 행 이(가) 삭제되었습니다.
SELECT*FROM DEPT;
ROLLBACK TO SAVEPOINT A;
--롤백 완료.
SELECT * FROM DEPT;
------------------------------------------------------------------------
CREATE USER TEST IDENTIFIED BY 12345;
ALTER USER TEST IDENTIFIED BY 1234;
DROP USER TEST CASCADE;
--User TEST이(가) 삭제되었습니다.
------------------------------------------------------------------------
CREATE USER TEST IDENTIFIED BY 12345;

GRANT CREATE SESSION TO TEST;
--Grant을(를) 성공했습니다.

GRANT CREATE TABLE TO TEST;

GRANT CREATE SEQUENCE TO TEST;

GRANT CREATE VIEW TO TEST;

GRANT CONNECT, RESOURCE TO TEST;

REVOKE CREATE SEQUENCE FROM TEST;



